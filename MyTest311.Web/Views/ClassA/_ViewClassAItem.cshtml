@model MyTest311.Web.Models.ClassAModel

<script src='@this.Url.Content("~/Scripts/modal.js")'></script>
<script src='@this.Url.Content("~/Scripts/editCollection.js")'></script>

<table class="table table-striped table-bordered table-hover table_form" id="ClassAItem" >
    <thead>
        <tr>
	<th class="text-left">Name</th>
	<th class="text-left">Description</th>
	<th>Actions</th>
        </tr>
    </thead>
    <tbody>
    @if (Model.ClassAItem != null && Model.ClassAItem.Count > 0)
    {
        for (int i = 0; i < Model.ClassAItem.Count; i++)
        {
           <tr class="data_tr">
                <td class="text-left">
                    @Html.TextBoxFor(p => Model.ClassAItem[i].Name, new { @class = "readonly", @readonly = "readonly" })
                </td>
                <td class="text-left">
                    @Html.TextBoxFor(p => Model.ClassAItem[i].Description, new { @class = "readonly", @readonly = "readonly" })
                </td>
                <td class="center">
                    @Html.ActionLink(" ", "#", "#", new { href = "javascript:void(0);", @class = "m_details", title = "Details", onclick = "editClassAItem.viewItem(this)" })
                </td>
            </tr>
        }
    }
    </tbody>
</table>

<div class="mask" id="editClassAItem">
    <div class="product_catalog">
        <div class="title">
            <button type="button" class="close" onclick="MetaShare.Modal.prototype.closeModal('editClassAItem') " style="right: 1%;">&times; </button>
            <h4 style="width: 100%; margin: 0px">ClassAItem</h4>
        </div>
        <div class="border_line modal_view modal_scroll" id="editClassAItemContent">
             @Html.Partial("_ViewOneClassAItem", Model)
        </div>
    </div>
</div>

